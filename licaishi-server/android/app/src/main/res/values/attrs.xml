<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="MyEditTextAttr">
        <attr name="input_type" format="integer" />
        <attr name="max_length" format="integer" />
        <attr name="has_eye" format="boolean" />
        <attr name="hint" format="string" />
        <attr name="is_phone" format="boolean" />
        <attr name="line_margin_top" format="dimension" />
        <attr name="has_delete" format="boolean" />

        <attr name="allow_blank" format="boolean" />
        <attr name="allow_chinese" format="boolean" />
        <attr name="blank_eye" format="boolean" />
        <attr name="judge_content" format="boolean" />
    </declare-styleable>


    <declare-styleable name="ItemBar">
        <attr name="left_title" format="reference" />
        <attr name="right_title" format="reference" />
        <attr name="left_message" format="reference" />
        <attr name="right_message" format="reference" />
        <attr name="left_image" format="reference" />
        <attr name="right_image" format="reference" />
        <attr name="right_image_background" format="reference" />
        <attr name="left_image_background" format="reference" />
    </declare-styleable>

</resources>